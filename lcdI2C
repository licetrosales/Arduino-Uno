// Bibliotheken
#include <LiquidCrystal_I2C.h>
// LCD-Adresse 0x27, 16 Zeichen, 4 Zeilen
LiquidCrystal_I2C lcd(0x27, 16, 4);


void setup() {
  // put your setup code here, to run once:
  lcd.init();
  lcd.backlight();

}

void loop() {
  // put your main code here, to run repeatedly:
  lcd.setCursor(0,0);
  lcd.print("Anzeigetest");
  lcd.setCursor(0,1);
  lcd.print("1");
  lcd.setCursor(1,2);
  lcd.print("2");
  lcd.setCursor(1,3);
  lcd.print("3");
   
}
